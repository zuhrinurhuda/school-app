<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>STUDENT DATA</title>
  </head>
  <body>
    <h1>STUDENT DATA</h1>
    
    <table>
      <tr>
        <td><a href="/">Home</a></td>
        <td><a href="/teacher">Teachers</a></td>
        <td><a href="/student">Students</a></td>
        <td><a href="/subject">Subjects</a></td>
      </tr>
    </table>

    <form action="/student/edit/<%= dataStudents.id %>" method="POST">
      First name:<br>
      <input type="text" name="firstName" value="<%= dataStudents.firstName %>">
      <br>
      Last name:<br>
      <input type="text" name="lastName" value="<%= dataStudents.lastName %>">
      <br>
      Email:<br>
      <input type="text" name="email" value="<%= dataStudents.email %>">
      <br><br>
      <input type="submit" value="Submit">
    </form> 
  </body>
</html>
