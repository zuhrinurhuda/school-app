<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>SUBJECTS DATA</title>
  </head>
  <body>
    <h1>SUBJECTS DATA</h1>
    <table>
      <tr>
        <td><a href="/">Home</a></td>
        <td><a href="/teacher">Teachers</a></td>
        <td><a href="/student">Students</a></td>
        <td><a href="/subject">Subjects</a></td>
      </tr>
    </table>

   <table border="1px" ">
    <tr>
      <th>ID</th>
      <th>Subject Name</th>
      <th>Teacher</th>
      <th>Actions</th>
    </tr>
    <% dataSubjects.forEach((row) => { %>
      <tr>
        <td><%= row.id %></td>
        <td><%= row.subjectName %></td>
        <td>
          <ol style="margin: 0">
          <% for(let i=0; i<dataSubjects.length; i++) { %>
          <li><%= row.Teachers[i].firstName %></li>
          <% } %>
          </ol> 
        </td>
        <td><a href="/subject/enrolledstudents/<%= row.id %>">Enrolled Students</a></td>
      </tr>
    <% })%>
  </table> 

  <h4><a href="/subject/add">Add Subject</a></h4>
  </body>
</html>
