<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>SUBJECTS DATA</title>
  </head>
  <body>
    <h1>SUBJECTS DATA</h1>
    <table>
      <tr>
        <td><a href="/">Home</a></td>
        <td><a href="/teacher">Teachers</a></td>
        <td><a href="/student">Students</a></td>
        <td><a href="/subject">Subjects</a></td>
      </tr>
    </table>

   <table border="1px" ">
    <tr>
      <th>ID</th>
      <th>Subject Name</th>
      <th>Option</th>
    </tr>
    <% dataSubjects.forEach((row) => { %>
      <tr>
        <td><%= row.id %></td>
        <td><%= row.subjectName %></td>
        <td>
          <a href="/subject/edit/<%= row.id %>">Edit</a>
          <a> | </a>
          <a href="/subject/delete/<%= row.id %>">Delete</a>
        </td>
      </tr>
    <% })%>
  </table> 
  </body>
</html>
